{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowImportingTsExtensions": true,
        "strict": true,
        "noEmit": true,
        "allowJs": true,
        "checkJs": false,
        "jsx": "preserve"
    },
    "vueCompilerOptions": {
        "extensions": [
            ".vue",
            ".tsx",
            ".ts",
            ".jsx",
            ".js"
        ]
    }
}